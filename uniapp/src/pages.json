{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^cu-(.*)": "@/components/cu-$1/index.vue"
    }
  },

  "pages": [
    {
      "path": "pages/strawberry/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/donut/index",
      "style": {
        "navigationBarTitleText": "甜甜圈"
      }
    },
    {
      "path": "pages/watermelon/index",
      "style": {
        "navigationBarTitleText": "西瓜"
      }
    },
    {
      "path": "pages/watermelon/faq/index",
      "style": {
        "navigationBarTitleText": "常见问题"
      }
    },
    {
      "path": "pages/watermelon/settings/index",
      "style": {
        "navigationBarTitleText": "设置"
      }
    },
    {
      "path": "pages/watermelon/settings/agreement/index",
      "style": {
        "navigationBarTitleText": "用户协议"
      }
    },
    {
      "path": "pages/watermelon/settings/privacy/index",
      "style": {
        "navigationBarTitleText": "隐私政策"
      }
    },
    {
      "path": "pages/watermelon/settings/network/index",
      "style": {
        "navigationBarTitleText": "网络检测"
      }
    },
    {
      "path": "pages/login/index",
      "style": {
         "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/watermelon/profile/index",
      "style": {
        "navigationBarTitleText": "个人中心"
      }
    },
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "vue-uniapp-template",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "color": "#474747",
    "selectedColor": "#9899ff",
    "backgroundColor": "#fff",
    "list": [
      {
        "pagePath": "pages/strawberry/index",
        "text": "草莓",
        "iconPath": "static/images/tabbar/strawberry.png",
        "selectedIconPath": "static/images/tabbar/strawberry.png"
      },
      {
        "pagePath": "pages/donut/index",
        "text": "甜甜圈",
        "iconPath": "static/images/tabbar/donut.png",
        "selectedIconPath": "static/images/tabbar/donut.png"
      },
      {
        "pagePath": "pages/watermelon/index",
        "text": "西瓜",
        "iconPath": "static/images/tabbar/watermelon.png",
        "selectedIconPath": "static/images/tabbar/watermelon.png"
      }
    ]
  }
}
